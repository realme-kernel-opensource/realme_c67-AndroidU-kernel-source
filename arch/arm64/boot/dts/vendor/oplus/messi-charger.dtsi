&qupv3_se0_spi {
	status = "disabled";
};

&qupv3_se1_i2c {
	status = "okay";
	qcom,clk-freq-out = <400000>;
	pinctrl-names = "default", "sleep", "pulldown", "pullup";
	pinctrl-1 = <&qupv3_se1_i2c_sleep>;
	pinctrl-2 = <&qupv3_se1_i2c_pulldown>;
	pinctrl-3 = <&qupv3_se1_i2c_pullup>;

	bq27541-battery@55 {
		status = "okay";
		compatible = "oplus,bq27541-battery";
		reg = <0x55>;
		qcom,modify-soc-smooth;
		//qcom,battery-full-param;//only for wite battery full param in guage dirver probe on 7250 platform
	};

	sgm7220:sgm7220@47 {
		status = "okay";
		compatible = "oplus,sgm7220";
		reg = <0x47>;
		interrupt-parent = <&tlmm>;
		interrupts = <89 0>;
		interrupt-names = "sgm7220_int_irq";
		sgm7220,irq_gpio = <&tlmm 89 0x8008>;
		//sgm7220,reset-gpio = <&tlmm 12 0x0>;

		//typec irq
		pinctrl-names = "typec_inter_active", "typec_inter_sleep";
		pinctrl-0 = <&typec_inter_active>;
		pinctrl-1 = <&typec_inter_sleep>;
	};

	wusb3801@60 {
		status = "okay";
		compatible = "oplus,wusb3801x";
		reg = <0x60>;
		interrupt-parent = <&tlmm>;
		interrupts = <89 0>;
		interrupt-names = "wusb3801_int_irq";
		wusb3801,irq_gpio = <&tlmm 89 0x8008>;
		//sgm7220,reset-gpio = <&tlmm 12 0x0>;
		wusb3801,init-mode = <0x24>;
		wusb3801,host-current = <0x01>;
		wusb3801,drp-toggle-time = <40>;

		//typec irq
		pinctrl-names = "typec_inter_active", "typec_inter_sleep";
		pinctrl-0 = <&typec_inter_active>;
		pinctrl-1 = <&typec_inter_sleep>;
	};

 	bq25890h:bq25890h@6a {
 		status = "okay";
 		compatible = "ti,bq25890h";
 		reg = <0x6a>;

 		ti,sy6970,usb-vreg =<4400>;
 		ti,sy6970,precharge-current = <256>;
 		ti,sy6970,termination-current = <250>;
		ti,sy6970,boost-voltage = <5100>;
 		ti,sy6970,boost-current = <1200>;
 		ti,sy6970,charge-detect-enable;
		ti,sy6970,fastchg-cv-for-ovp = <4608>;
		ti,sy6970,disable-qc;
 
 		qcom,chg_irq_gpio = <&tlmm 105 0x00>;
 		dpdm-supply = <&usb2_phy0>;
 		interrupt-parent = <&tlmm>;
 		interrupts = <105 0>;

		pinctrl-names = "splitchg_inter_active","splitchg_inter_sleep";
		pinctrl-0 = <&splitchg_inter_active>;
		pinctrl-1 = <&splitchg_inter_sleep>;

 	};
};

/*load vibrator for rm pmic*/
&soc {
	qcom,vibrator@5700 {
			compatible = "qcom,qpnp-vibrator-ldo";
			reg = <0x5700 0x100>;
			qcom,vib-ldo-volt-uv = <3000000>;
			qcom,disable-overdrive;
			qcom,vib-type = <1>;	/*0:ldo 1:gpio*/
			status = "okay";

			qcom,vibrator_switch-gpio = <&tlmm 50 0x00>;
			pinctrl-names = "vib_on_high", "vib_off_low";
			pinctrl-0 = <&vib_on_high>;
			pinctrl-1 = <&vib_off_low >;
	};

	oplus,track-charge {
		compatible = "oplus,track-charge";
		track,fast_chg_break_t_thd = <1000>;
		track,general_chg_break_t_thd = <200>;
		track,voocphy_type = <2>;
		track,wired_max_power = <33000>;
		track,wired_fast_chg_scheme = <4>;
	};

	gpio_keys {
		vol_up {
			/delete-property/ label;
			label = "volume_down";
			/delete-property/ linux,code;
			linux,code = <KEY_VOLUMEDOWN>;
		};
	};
};

