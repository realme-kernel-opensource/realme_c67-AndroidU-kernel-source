&pm6125_vadc {
    pinctrl-0 = <&usbtemp_r_v_chan_default &usbtemp_l_v_chan_default &batt_id_chan_default &charge_therm_default>;

    usbtemp_r_v_chan {
        reg = <ADC5_GPIO1_100K_PU>;
        label = "usbtemp_r_v_chan";
        qcom,ratiometric;
        qcom,hw-settle-time = <200>;
        qcom,pre-scaling = <1 1>;
        qcom,scale-fn-type = <ADC_SCALE_HW_CALIB_DEFAULT>;
    };

    usbtemp_l_v_chan {
        reg = <ADC5_GPIO2_100K_PU>;
        label = "usbtemp_l_v_chan";
        qcom,ratiometric;
        qcom,hw-settle-time = <200>;
        qcom,pre-scaling = <1 1>;
        qcom,scale-fn-type = <ADC_SCALE_HW_CALIB_DEFAULT>;
    };

    batt_id_chan {
        reg = <ADC5_GPIO3_100K_PU>;
        label = "batt_id_chan";
        qcom,ratiometric;
        qcom,hw-settle-time = <200>;
        qcom,pre-scaling = <1 1>;
        qcom,scale-fn-type = <ADC_SCALE_HW_CALIB_DEFAULT>;
    };

    charge_therm {
        reg = <ADC5_GPIO4_100K_PU>;
        label = "charge_therm";
        qcom,ratiometric;
        qcom,hw-settle-time = <200>;
        qcom,pre-scaling = <1 1>;
        qcom,scale-fn-type = <ADC_SCALE_HW_CALIB_DEFAULT>;
    };

};

&pm6125_gpios {
	pm6125_chg_pins: pm6125_chg_pins {
        usbtemp_r_v_chan {
            usbtemp_r_v_chan_default: usbtemp_r_v_chan_default {
                pins = "gpio3";
                bias-high-impedance;
            };
        };

        usbtemp_l_v_chan {
            usbtemp_l_v_chan_default: usbtemp_l_v_chan_default {
            	pins = "gpio4";
            	bias-high-impedance;
            };
        };

        batt_id_chan {
            batt_id_chan_default: batt_id_chan_default {
                pins = "gpio6";
                bias-high-impedance;
           };
        };

        charge_therm {
            charge_therm_default: charge_therm_default {
                pins = "gpio7";
                bias-high-impedance;
            };
        };
	};
};

&pm6125_adc_tm_iio {

	charge_therm {
		reg = <2>;
		io-channels = <&pm6125_vadc ADC5_GPIO4_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
    };
};

